        "from qiskit import QuantumCircuit, transpile\n",
        "from qiskit_ibm_runtime import QiskitRuntimeService, Sampler\n",
        "\n",
        "service = QiskitRuntimeService(channel=\"ibm_quantum_platform\", token=\"3z61wK5_t67nHUhuKRHJwXAIzqRJVAmJ_IIKmiS7Zb0_\")\n",
        "backend = service.least_busy(operational=True, simulator=False, min_num_qubits=1)  # Using least_busy instead of get_backend\n",
        "\n",
        "delays = [100, 500, 1000]  # µs\n",
        "jobs = []\n",
        "\n",
        "for delay in delays:\n",
        "    # Baseline\n",
        "    qc_base = QuantumCircuit(1, 1)\n",
        "    qc_base.h(0)\n",
        "    qc_base.delay(delay, unit='us')\n",
        "    qc_base.h(0)\n",
        "    qc_base.measure(0, 0)\n",
        "\n",
        "    # Isolated\n",
        "    qc_iso = QuantumCircuit(1, 1)\n",
        "    qc_iso.h(0)\n",
        "    qc_iso.delay(delay, unit='us')\n",
        "    qc_iso.h(0)\n",
        "    qc_iso.reset(0)\n",
        "    qc_iso.h(0)\n",
        "    qc_iso.measure(0, 0)\n",
        "\n",
        "    # Transpile e roda\n",
        "    base_t = transpile(qc_base, backend)\n",
        "    iso_t = transpile(qc_iso, backend)\n",
        "\n",
        "    sampler = Sampler(mode=backend)\n",
        "    job_base = sampler.run([base_t], shots=8192)\n",
        "    job_iso = sampler.run([iso_t], shots=8192)\n",
        "\n",
        "    jobs.append((delay, job_base.job_id(), job_iso.job_id()))\n",
        "    print(f\"Delay {delay}µs → Base: {job_base.job_id()}, Iso: {job_iso.job_id()}\")\n",
        "\n",
        "# Salve os job_ids e me manda depois!"
      ],
