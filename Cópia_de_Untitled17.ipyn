        "import sys\n",
        "!{sys.executable} -m pip install qiskit qiskit-ibm-runtime\n",
        "\n",
        "from qiskit import QuantumCircuit, transpile\n",
        "from qiskit_ibm_runtime import QiskitRuntimeService, Sampler\n",
        "import numpy as np\n",
        "\n",
        "# Inicializa com seu token (você roda localmente)\n",
        "service = QiskitRuntimeService(channel=\"ibm_quantum_platform\",token= \"3z61wK5_t67nHUhuKRHJwXAIzqRJVAmJ_IIKmiS7Zb0_\")\n",
        "\n",
        "# Circuito Baseline: |+> com delay\n",
        "qc_baseline = QuantumCircuit(1)\n",
        "qc_baseline.h(0)\n",
        "qc_baseline.delay(1000, unit='us')  # ~4x T2 típico\n",
        "qc_baseline.h(0)\n",
        "qc_baseline.measure_all()\n",
        "\n",
        "# Circuito π + 0: Projeção no subespaço\n",
        "qc_isolated = QuantumCircuit(1)\n",
        "qc_isolated.h(0)\n",
        "qc_isolated.delay(1000, unit='us')\n",
        "# Projeção: reset para |+> se sair do subespaço (aproximação prática)\n",
        "qc_isolated.h(0)\n",
        "qc_isolated.reset(0)\n",
        "qc_isolated.h(0)\n",
        "qc_isolated.measure_all()\n",
        "\n",
        "# Transpile para backend real (escolha um com T2 > 200µs)\n",
        "backend = service.least_busy(operational=True, simulator=False, min_num_qubits=1)\n",
        "qc_baseline_t = transpile(qc_baseline, backend)\n",
        "qc_isolated_t = transpile(qc_isolated, backend)\n",
        "\n",
        "# Roda com Sampler\n",
        "sampler = Sampler(mode=backend)\n",
        "job_baseline = sampler.run([qc_baseline_t], shots=8192)\n",
        "job_isolated = sampler.run([qc_isolated_t], shots=8192)"
      ]
    },
